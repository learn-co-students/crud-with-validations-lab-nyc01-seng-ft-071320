<div>
<%=form_for @song do |f|%>
    <%=f.label :title%>
    <%=f.text_field :title%><br>

    <p>
    <%=f.label :released%>
    <%=f.check_box :released%><br>
    </p>

    <%=f.label :release_year%>
    <%=f.text_field :release_year%><br>

    <p>
    <%=f.label :artist_name%>
    <%=f.text_field :artist_name%><br>
    </p>

    <%=f.label :genre%>
    <%=f.text_field :genre%><br>
    
    <p>
    <%=f.submit%>
    </p>
<%end%>
</div>
