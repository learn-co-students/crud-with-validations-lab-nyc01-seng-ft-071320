<h1>Songs</h1>
<table>
  <% @songs.each do |song| %>
    <tbody>
      <tr>
        <td><%= link_to song.title, song_path(song) %></td>
        <td>Released Year: <%= song.release_year %></td>
        <td>Genre: <%= song.genre %></td>
        <td>Artist: <%= song.artist_name %></td>
      </tr>
    </tbody>
  <% end %>
</table>